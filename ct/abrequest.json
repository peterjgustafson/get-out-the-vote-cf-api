{
    "form": {
        "headerText": "Request An Absentee Ballot",
        "submit": "Next",
        "submitText": "You are one step away from casting your vote, click next to continue.",
        "formStep": [
        {
            "-id": "1",
            "headerText": "Let's Get Started",
            "formSection": {
            "-id": "1",
              "fieldset": {
                    "field": [
                          {
                            "-id": "fname",
                            "-type": "string",
                            "-required": "true",
                            "-label": "First Name"
                          },
                          {
                            "-id": "mname",
                            "-type": "string",
                            "-required": "false",
                            "-label": "Middle Name"
                          },
                          {
                            "-id": "lname",
                            "-type": "string",
                            "-required": "true",
                            "-label": "Last Name",
                            "-width": "2X"
                          },
                          {
                            "-id": "sfx",
                            "-type": "string",
                            "-required": "false",
                            "-label": "Suffix",
                            "-width": "1X"
                          },
                          {
                            "-id": "email",
                            "-type": "email",
                            "-required": "true",
                            "-label": "Email Address",
                            "-width": "2X"
                          },
                          {
                            "-id": "step1zip",
                            "-type": "numeric",
                            "-required": "true",
                            "-label": "ZIP Code",
                            "-maxLength": "5",
                            "-minLength": "5",
                            "-width": "1X",
                            "mask": "00000"
                          }
                        ]
                    }
                }
            },
            {
                "-id": "2",
                "-hidden": "true",
                "-type": "confirm",
                "headerText": "Confirm Information",
                "headerText": "Home Address",
                "source": "voterDB",
                "pre": "Is the following information correct?",
                "post": "By clicking correct, I confirm that the above information is complete and accurate.",
                "confirmText": "Correct",
                "editText": "Incorrect",
                "formSection": {
                    "-id": "1",
                    "fieldset": {
                        "field": [
                          {
                            "-id": "raddr1",
                            "-type": "string",
                            "-required": "true",
                            "-label": "Address",
                            "-width": "full"
                          },
                          {
                            "-id": "raddr2",
                            "-type": "string",
                            "-required": "false",
                            "-label": "Address Line 2",
                            "-width": "full"
                          },
                          {
                            "-id": "city",
                            "-type": "string",
                            "-required": "true",
                            "-label": "City"
                          },
                          {
                            "-id": "rzip5",
                            "-type": "numeric",
                            "-required": "true",
                            "-label": "ZIP Code",
                            "-maxLength": "5",
                            "-minLength": "5",
                            "mask": "00000",
                            "-width": "1X"
                          },
                          {
                            "-id": "cust_county",
                            "-type": "string",
                            "-required": "true",
                            "-label": "County",
                            "-width": "1X"
                          },
                          {
                            "-id": "cust_dob",
                            "-type": "date",
                            "-required": "true",
                            "-label": "Date of Birth",
                            "-width": "1X"
                          },
                          {
                            "-id": "tel_num",
                            "-type": "phone",
                            "-required": "true",
                            "-label": "Phone",
                            "-width": "1X"
                          }
                        ]
                      }
                }
            },
            {
            "-id": "3",
            "-hidden": "true",
            "headerText": "Mailing Address",
            "formSection": [
                {
                    "-id": "1",
                    "fieldset": {
                        "field": [
                            {
                              "-id": "deliverto",
                              "-type": "list",
                              "-required": "true",
                              "-label": "Where would you like your Absentee Ballot Mailed?",
                              "listItems": {
                                "listItem": [
                                  {
                                    "-value": "home",
                                    "#Text": "To my home address",
                                    "-triggers": {
                                      "2": "hide"
                                    }
                                  },
                                  {
                                    "-value": "mailAddress",
                                    "#Text": "To a different mailing address",
                                    "-triggers": {
                                      "2": "show"
                                    }
                                  }
                                ]
                              }
                            }
                        ]
                      }
                    },
                {
                    "-id": "2",
                    "-hidden": "true",
                    "fieldset": {
                        "field": [
                          {
                            "-id": "maddr1",
                            "-type": "string",
                            "-required": "true",
                            "-label": "Address",
                            "-width": "full"
                          },
                          {
                            "-id": "maddr2",
                            "-type": "string",
                            "-required": "false",
                            "-label": "Address Line 2",
                            "-width": "full"
                          },
                          {
                            "-id": "mcity",
                            "-type": "string",
                            "-required": "true",
                            "-label": "City"
                          },
                          {
                            "-id": "msta",
                            "-type": "string",
                            "-required": "true",
                            "-label": "State Abbr",
                            "-maxLength": "2",
                            "-minLength": "2",
                            "-width": "1X"
                          },
                          {
                            "-id": "mzip5",
                            "-type": "numeric",
                            "-required": "true",
                            "-label": "ZIP Code",
                            "-maxLength": "5",
                            "-minLength": "5",
                            "-width": "1X",
                            "mask": "00000"
                          }
                        ]
                      }
                    }
                ]
            }
        ]
    }
}